{
    "version": 2,
    "installCommand": "pnpm install",
    "buildCommand": "pnpm db:generate",
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api"
        }
    ]
}