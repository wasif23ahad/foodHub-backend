{
    "version": 2,
    "installCommand": "pnpm install && pnpm db:generate",
    "buildCommand": "pnpm build",
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ]
}