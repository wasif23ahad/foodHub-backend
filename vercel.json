{
    "version": 2,
    "installCommand": "pnpm install",
    "buildCommand": "pnpm db:generate && pnpm build",
    "outputDirectory": "dist",
    "functions": {
        "dist/server.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/dist/server.js"
        }
    ]
}