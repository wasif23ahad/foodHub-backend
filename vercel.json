{
    "version": 2,
    "framework": null,
    "installCommand": "pnpm install && pnpm db:generate",
    "buildCommand": "pnpm build",
    "outputDirectory": "dist",
    "functions": {
        "dist/server.js": {
            "runtime": "nodejs20.x"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/server.js"
        }
    ]
}